{
  "name": "react-layered-chart",
  "version": "0.0.1",
  "description": "A high-performance canvas-based time series visualization in React.",
  "main": "lib/index.js",
  "author": "Sean Kelley <seansfkelley@gmail.com>",
  "license": "none yet",
  "repository": {
    "type": "git",
    "url": "https://github.com/seansfkelley/react-layered-chart.git"
  },
  "scripts": {
    "start": "./node_modules/http-server/bin/http-server -c-1",
