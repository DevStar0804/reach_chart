{
  "name": "react-layered-chart",
  "version": "0.2.1",
  "description": "A high-performance canvas-based time series visualization in React.",
  "main": "lib/index.js",
  "author": "Sean Kelley <seansfkelley@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/seansfkelley/react-layered-chart.git"
  },
  "scripts": {
    "dev:js": "./node_modules/.bin/webpack-dev-server --inline --open",
    "dev:css": "./node_modules/.bin/stylus -o dist -w -m styles",
    "test": "multi=\"mocha-osx-reporter=- spec=-\" ./node_modules/.bin/mocha --reporter mocha-multi",
    "build:lib": "./node_modules/.bin/babel src/ -d lib/",
    "build:dist": "./node_modules/.bin/webpack && ./node_modules/.bin/stylus styles/index.styl -o dist",
    "prepublish": "npm run build:lib && npm run build:dist"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-class-properties",
      "transform-decorators-legacy"
    ]
  },
  "devDependencies": {
    "babel": "^6.3.13",
    "babel-cli": "^6.4.0",
    "babel-core": "^6.3.17",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-class-properties": "^6.3.13",
    "babel-plugin-transform-decorators-legacy": "^1.3.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "memoizee": "^0.3.9",
    "mocha": "^2.3.4",
    "mocha-multi": "^0.7.2",
    "mocha-osx-reporter": "^0.1.2",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "redux": "^3.0.5",
    "redux-thunk": "^1.0.2",
    "should": "^8.0.2",
    "sinon": "^1.17.2",
    "stylus": "^0.53.0",
    "webpack": "^1.12.11",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "classnames": "^2.2.1",
    "d3-ease": "^0.3.1",
    "d3-interpolate": "^0.3.0",
    "d3-scale": "^0.3.0",
    "lodash": "^4.0.0",
    "pure-render-decorator": "^0.2.0"
  },
  "peerDependencies": {
    "react": "^0.14.3",
    "react-dom": "^0.14.3"
  },
  "keywords": [
    "timeline",
    "time",
    "series",
    "chart",
    "canvas",
    "react",
    "component"
  ]
}
