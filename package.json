      "transform-class-properties",
      "transform-decorators-legacy"
    ]
  },
  "devDependencies": {
    "babel": "^6.3.13",
    "babel-core": "^6.3.17",
    "babel-plugin-transform-class-properties": "^6.3.13",
    "babel-plugin-transform-decorators-legacy": "^1.3.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-register": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.1.1",
    "gulp-concat": "^2.6.0",
    "gulp-filter": "^3.0.1",
    "gulp-jsx": "^2.0.1",
    "gulp-livereload": "^3.8.1",
    "gulp-notify": "^2.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-stylus": "^2.1.1",
    "http-server": "^0.8.5",
    "mocha": "^2.3.4",
    "mocha-multi": "^0.7.2",
    "mocha-osx-reporter": "^0.1.2",
    "should": "^8.0.2",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.6.1"
  },
  "dependencies": {
    "classnames": "^2.2.1",
    "d3-ease": "^0.3.1",
    "d3-interpolate": "^0.3.0",
    "d3-scale": "^0.3.0",
    "lodash": "^3.10.1",
    "pure-render-decorator": "^0.2.0",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "redux": "^3.0.5",
    "redux-thunk": "^1.0.2"
